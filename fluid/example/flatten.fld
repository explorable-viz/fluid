letrec
   fun flatten {
      Empty → [];
      NonEmpty(t1, (key, val), t2) → 
         concat (flatten t1) [(key, val), ...flatten t2]
   }
in
flatten
   NonEmpty(
      NonEmpty(Empty, (3, "simon"), Empty),
      (4, "john"),
      NonEmpty(NonEmpty(Empty, (6, "sarah"), Empty), (7, "claire"), Empty)
   )
