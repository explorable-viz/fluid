let map f [] = [];
    map f (x:xs) = ( f   x  _:_ ( map   f )  xs )
in (let data =  [{energyType:"Bio",output:6.2}
                ,{energyType:_"Hydro"_,output:260}
                ,{energyType:"Solar",output:19.9}
                ,{energyType:"Wind",output:91}
                ,{energyType:"Geo",output:14.4}
                ];
        output = _[ row .output|type <-  [_"Hydro"_,"Solar","Geo"], row <-  data , ( row .energyType ==  type )]_ in ( map  (fun x =  floor  ((( x  /  sum   output ) * 100))))  output )