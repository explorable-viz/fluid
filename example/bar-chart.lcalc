let
   data = Cons((2001, 15), Cons((2002, 8), Cons((2005, 22), Cons((2007, 9), Nil))))
in
let width = 6 in
let sep = width + 1 in
let rects = map (fun (year, age) → Rect(width, age)) data in
Graphic(
   Cons(Graphic(axis2 1 5 7),
   Cons(Graphic(map (fun ps → PathStroke(map transpose ps)) (axis 1 (length rects) width)),
   Cons(Graphic(spaceRight' sep (map renderRect rects)), Nil)))
)
