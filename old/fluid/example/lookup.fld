letrec
   fun lookup k {
      Empty → None;
      NonEmpty(t1, (key, val), t2) → 
         match compare (k, key) as {
            LT → lookup k t1;
            EQ → Some(val);
            GT → lookup k t2
         }
   }
in
lookup
   6
   NonEmpty(
      NonEmpty(Empty, (3, "simon"), Empty),
      (4, "john"),
      NonEmpty(NonEmpty(Empty, (6, "sarah"), Empty), (7, "claire"), Empty)
   )
